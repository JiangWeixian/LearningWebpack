{
  "name": "@aiou/chrome-extenstion-template",
  "version": "1.0.3",
  "description": "chrome-extenstion-template",
  "main": "index.js",
  "scripts": {
    "start": "npm ci",
    "dev": "webpack --config webpack/webpack.dev.js --watch",
    "build": "webpack --config webpack/webpack.prod.js",
    "clean": "npx rimraf dist",
    "prettier": "prettier --config .prettierrc --write '**/*'",
    "update": "npm update && npm update --save-dev",
    "check": "npm outdated && npm outdated --save-dev",
    "prepublishOnly": "np --no-cleanup --yolo --no-publish --any-branch"
  },
  "author": "JW",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JiangWeixian/templates/tree/master/packages/chrome-extenstion-template"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "jquery": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rxjs": "^6.5.2",
    "rxjs-hooks": "^0.5.1"
  },
  "devDependencies": {
    "@types/chrome": "0.0.86",
    "@types/jquery": "^3.3.30",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "copy-webpack-plugin": "^5.0.3",
    "crx-hotreload": "^1.0.4",
    "husky": "^3.0.1",
    "np": "^5.0.3",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.4.5",
    "typescript": "~3.4.5",
    "webpack": "~4.17.2",
    "webpack-cli": "~3.1.0",
    "webpack-merge": "~4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
