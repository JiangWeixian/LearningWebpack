    "vuepress": "^0.14.10"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "tsc -p tsconfig.json && node docs/Menus/generator.js && node -r tsconfig-paths/register docs/.vuepress/config.js",
