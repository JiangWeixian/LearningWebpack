{
  "name": "docs-templates",
  "version": "1.0.0",
  "description": "vuepress template",
  "main": "docs/index.ts",
  "dependencies": {
    "markdown-it-task-checkbox": "^1.0.6"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
