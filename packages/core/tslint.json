{
  "extends": ["tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "prettier": true,
    "no-floating-promises": false,
    "no-any": true,
    "no-implicit-dependencies": [true, "@"],
    "jsx-no-lambda": false,
    "react-this-binding-issue": false,
    "no-use-before-declare": false,
    "jsx-key": false
  }
}
