{
  "name": "nextjs-rematch2-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "next -p 3001",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "prettier": "pretty-quick --staged",
    "mock": "PORT=8080 umi-serve",
    "watch_mock": "npm-watch mock"
  },
  "watch": {
    "mock": {
      "patterns": [
        "mock"
      ],
      "extensions": "js",
      "quiet": true
    }
  },
  "dependencies": {
    "@rematch2/core": "^0.2.0-alpha.0",
    "axios": "0.19.0",
    "http-proxy-middleware": "^0.20.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "latest",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-redux": "^7.1.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "12.7.8",
    "@types/react": "16.9.3",
    "@types/react-dom": "16.9.1",
    "@types/react-redux": "^7.1.1",
    "cross-env": "^6.0.3",
    "husky": "3.1.0",
    "npm-watch": "^0.6.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.2",
    "umi-serve": "1.9.3"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
